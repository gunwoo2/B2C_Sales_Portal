<mvc:View xmlns:form="sap.ui.layout.form"
	controllerName="sync.ea.company1.controller.settings.MasterSettings"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns="sap.m"
	xmlns:layout="sap.ui.layout"
	xmlns:microchart="sap.suite.ui.microchart">
    <Carousel class="sapUiContentPadding">
		<Page>
			<layout:BlockLayout
			id="BlockLayout"
			background="Default">
			<layout:BlockLayoutRow>
				<layout:BlockLayoutCell class="verticalLayoutWithBackground13">
					<!-- 배경 이미지가 표시될 부분 -->
					<layout:VerticalLayout>
						<!-- <Title text="ISOT"  level="H3" titleStyle="H2" class="TOPTEXT"/> -->
						<Text text="프리미엄 등급 타이어"  class="TOPSUBTEXT font-face2 overlay-text2"/>
						<!-- <Text text="Partnering Progress"  class="TOPSUBTEXT font-face"/> -->
					</layout:VerticalLayout>
				</layout:BlockLayoutCell>
			</layout:BlockLayoutRow>
			</layout:BlockLayout>
	
			<layout:VerticalLayout class="sapUiResponsiveMargin">

				
			<layout:Grid defaultSpan="XL6 L6 M6 S12">
				
				<!-- 가솔린 사계절 프리미엄 타이어 -->            
				<layout:VerticalLayout >
				<SlideTile id="slideTile02" class="sapUiTinyMarginBegin sapUiTinyMarginTop">
					<GenericTile
						id="slideTile02_1"
						backgroundImage="{/TileCollection2/0/backgroundImage}"
						frameType="TwoByOne"
						press="pressOnTileOne"
						class="customGenericTile">
					</GenericTile>
					<GenericTile
						id="slideTile02_2"
						backgroundImage="{/TileCollection4/1/backgroundImage}"
						frameType="TwoByOne"
						press="pressOnTileTwo"
						class="customGenericTile">
						<TileContent footer="판매가격 : 167,200원 부터">
							<NewsContent
								contentText="Kinergy AS EV"
								subheader="가능인치 : 18' / 19' / 20'" />
						</TileContent>
					</GenericTile>   
				</SlideTile>
					<layout:HorizontalLayout>
						<Image src="	https://www.hankooktire.com/content/dam/hankooktire/kr/product-/bi/pcr/bi_Ventus_S1_evo_Z.svg" class="sapUiTinyMarginBegin sapUiTinyMarginTop image-container"/>
					</layout:HorizontalLayout>
						<Text text="프리미엄을 넘어선 특별한 가치"  class="sapUiTinyMarginBegin sapUiTinyMarginTop"/>
				
					<layout:HorizontalLayout>
						<Button text="가솔린 타이어" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton" />
						<Button text="올시즌" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton" />
						<Button text="내구성" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton" />
						<Button text="승차감" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton" />
					</layout:HorizontalLayout>
					<layout:HorizontalLayout>						
						<Button text="마른노면성능" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton" />
						<Button text="연비(구름저항)" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton" />
						<Button text="Premium" class="sapUiTinyMarginBegin sapUiTinyMarginTop customPreButton "/>
					</layout:HorizontalLayout>
				</layout:VerticalLayout>


                <!-- 가솔린 윈터 프리미엄 타이어 -->
                <layout:VerticalLayout >
					<SlideTile id="slideTile4" class="sapUiTinyMarginBegin sapUiTinyMarginTop customSlideTile">						<GenericTile
							id="slideTile4_1"
							backgroundImage="{/TileCollection4/0/backgroundImage}"
							frameType="TwoByOne"
							press="pressOnTileOne"
							class="customGenericTile">
						</GenericTile>
						<GenericTile
							id="slideTile4_2"
							backgroundImage="{/TileCollection4/1/backgroundImage}"
							frameType="TwoByOne"
							press="pressOnTileTwo"
							class="customGenericTile">
							<TileContent footer="판매가격 : 136,000원 부터">
								<NewsContent
									contentText="UltraContact UC 6"
									subheader="가능인치 : 18' / 19' / 20'" />
							</TileContent>
						</GenericTile>                
					</SlideTile>
					<layout:HorizontalLayout>
							<Image src="https://asset.hankooktire.com/content/dam/hankooktire/local/img/product/w330/logo1.svg" class="sapUiTinyMarginBegin sapUiTinyMarginTop image-container"/>
						</layout:HorizontalLayout>
							<Text text="유럽에서 인정받은 겨울철 고성능 퍼포먼스 타이어"  class="sapUiTinyMarginBegin sapUiTinyMarginTop"/>
					
						<layout:HorizontalLayout>
						<Button text="가솔린 타이어" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton" />
						<Button text="빙판길성능" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton"/>
                  		<Button text="눈길성능" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton"/>
					</layout:HorizontalLayout>
					<layout:HorizontalLayout>						
						<Button text="내구성" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton"/>
						<Button text="연비(구름저항)" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton" />
						<Button text="겨울용" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButtonWinter" />
						<Button text="Premium" class="sapUiTinyMarginBegin sapUiTinyMarginTop customPreButton "/>
					</layout:HorizontalLayout>
				</layout:VerticalLayout>

			</layout:Grid>


			
						

			<layout:Grid defaultSpan="XL6 L6 M6 S12">

				<!-- 전기차 사계절 프리미엄 타이어 -->
				<layout:VerticalLayout >			
				<SlideTile id="slideTile10" class="sapUiTinyMarginBegin sapUiTinyMarginTop">
					<GenericTile
						id="slideTile10_1"
						backgroundImage="{/TileCollection10/0/backgroundImage}"
						frameType="TwoByOne"
						press="pressOnTileOne"
						class="customGenericTile">
					</GenericTile>
					<GenericTile
						id="slideTile10_2"
						backgroundImage="{/TileCollection10/1/backgroundImage}"
						frameType="TwoByOne"
						press="pressOnTileTwo"
						class="customGenericTile">
						<TileContent footer="판매가격 : 167,200원 부터">
							<NewsContent
								contentText="Kinergy AS EV"
								subheader="가능인치 : 18' / 19' / 20'" />
						</TileContent>
					</GenericTile>   
				</SlideTile>

				<layout:HorizontalLayout>
					<Image src="https://asset.hankooktire.com/content/dam/hankooktire/kr/product-/bi/pcr/bi_k127e_0425.svg" class="sapUiTinyMarginBegin sapUiTinyMarginTop image-container"/>
				</layout:HorizontalLayout>
					<Text text="프리미엄 전기차가 선택한 고성능 스포츠 타이어"  class="sapUiTinyMarginBegin sapUiTinyMarginTop"/>
			
				<layout:HorizontalLayout>
						<Button text="전기차" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButtonGreen"/>
						<Button text="올시즌" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton"/>
						<Button text="정숙성" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton"/>
						<Button text="승차감" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton"/>
				</layout:HorizontalLayout>
				<layout:HorizontalLayout>
						<Button text="연비(구름저항)" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton"/>
						<Button text="Premium" class="sapUiTinyMarginBegin sapUiTinyMarginTop customPreButton "/>
				</layout:HorizontalLayout>
				</layout:VerticalLayout>

                <!-- 전기차 윈터 프리미엄 타이엄 -->
				<layout:VerticalLayout >			
				<SlideTile id="slideTile12" class="sapUiTinyMarginBegin sapUiTinyMarginTop">
						<GenericTile
							id="slideTile12_1"
							backgroundImage="{/TileCollection12/0/backgroundImage}"
							frameType="TwoByOne"
							press="pressOnTileOne"
							class="customGenericTile">
						</GenericTile>
						<GenericTile
							id="slideTile12_2"
							backgroundImage="{/TileCollection12/1/backgroundImage}"
							frameType="TwoByOne"
							press="pressOnTileTwo"
							class="customGenericTile">
							<TileContent footer="판매가격 : 136,000원 부터">
								<NewsContent
									contentText="UltraContact UC 6"
									subheader="가능인치 : 18' / 19' / 20'" />
							</TileContent>
						</GenericTile>                
					</SlideTile>

				<layout:HorizontalLayout>
						<Image src="https://asset.hankooktire.com/content/dam/hankooktire/kr/product-/bi/pcr/bi_iON_icept0731new02.svg" class="sapUiTinyMarginBegin sapUiTinyMarginTop image-container"/>
					</layout:HorizontalLayout>
						<Text text="유럽에서 인정한 겨울철 고성능 퍼포먼스 전기차 타이어"  class="sapUiTinyMarginBegin sapUiTinyMarginTop"/>
				
					<layout:HorizontalLayout>
						<Button text="전기차" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButtonGreen"/>
						<Button text="M+S" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton"/>
                 		<Button text="내구성" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton"/>
						<Button text="연비(구름저항)" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton"/>
				</layout:HorizontalLayout>
				<layout:HorizontalLayout>
						
						<Button text="빙판길성능" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton"/>
                  		<Button text="눈길성능" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButton"/>
						<Button text="겨울용" class="sapUiTinyMarginBegin sapUiTinyMarginTop customButtonWinter" />
						<Button text="Premium" class="sapUiTinyMarginBegin sapUiTinyMarginTop customPreButton "/>
				</layout:HorizontalLayout>
				</layout:VerticalLayout>
			
			</layout:Grid>		

		


            <Title
            titleStyle="H2"
            text="Contact us"
            class="sapUiTinyMarginBegin sapUiMediumMarginTop"/>
        <layout:HorizontalLayout allowWrapping="true">
            <GenericTile
                header="E-mail"
                subheader="ISOT@ISOT.co.kr"
                mode="LineMode"
                class="sapUiTinyMarginBegin"/>
        </layout:HorizontalLayout>
    </layout:VerticalLayout>
</Page>
</Carousel>

</mvc:View>

